{
	"name": "medvision-backend",
	"version": "1.0.0",
	"description": "",
	"main": "src/server.ts",
	"scripts": {
		"dev": "tsx watch --env-file=.env src/server.ts",
		"build": "tsc",
		"start": "node --import tsx src/server.ts",
		"format": "biome format --write",
		"db:generate": "pnpm exec prisma generate",
		"db:migrate": "pnpm exec prisma migrate deploy",
		"db:migrate:dev": "pnpm exec prisma migrate dev",
		"db:push": "pnpm exec prisma db push",
		"db:studio": "pnpm exec prisma studio",
		"db:reset": "pnpm exec prisma migrate reset --force && pnpm run db:seed"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.20.0",
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@types/bcrypt": "^6.0.0",
		"@types/node": "^24.10.0",
		"@types/pg": "^8.15.6",
		"prisma": "^6.19.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@daily-co/daily-js": "^0.85.0",
		"@fastify/cookie": "^11.0.2",
		"@fastify/cors": "^11.1.0",
		"@fastify/jwt": "^10.0.0",
		"@fastify/swagger": "^9.6.0",
		"@prisma/adapter-pg": "^6.19.0",
		"@prisma/client": "^6.19.0",
		"@prisma/internals": "^6.19.0",
		"@scalar/fastify-api-reference": "^1.39.3",
		"bcrypt": "^6.0.0",
		"dotenv": "^17.2.3",
		"fastify": "^5.6.1",
		"fastify-plugin": "^5.1.0",
		"fastify-type-provider-zod": "^6.1.0",
		"pg": "^8.16.3",
		"postgres": "^3.4.7",
		"resend": "^6.5.2",
		"tsx": "^4.20.6",
		"zod": "^4.1.12"
	}
}
